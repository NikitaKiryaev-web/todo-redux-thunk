{
    "extends": [
      "react-app",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "jsx-quotes": [
        1,
        "prefer-double"
      ],
      "semi":"error",
      "comma-dangle": ["error", "always-multiline"],
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  }